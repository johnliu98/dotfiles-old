#!/bin/sh

time=$(date '+%I')

case "$time" in
	"00") timeicon="ğŸ•›" ;;
	"01") timeicon="ğŸ•" ;;
	"02") timeicon="ğŸ•‘" ;;
	"03") timeicon="ğŸ•’" ;;
	"04") timeicon="ğŸ•“" ;;
	"05") timeicon="ğŸ•”" ;;
	"06") timeicon="ğŸ••" ;;
	"07") timeicon="ğŸ•–" ;;
	"08") timeicon="ğŸ•—" ;;
	"09") timeicon="ğŸ•˜" ;;
	"10") timeicon="ğŸ•™" ;;
	"11") timeicon="ğŸ•š" ;;
	"12") timeicon="ğŸ•›" ;;
esac
dateicon="ğŸ“…"

date "+$dateicon %a %d %b %Y $timeicon %H:%M"
